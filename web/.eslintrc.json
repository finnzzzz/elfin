{
  "extends": [
    "next",
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins":[
    "@typescript-eslint"
  ],
  "rules":{
    "@typescript-eslint/no-unused-vars":  ["error", { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_"}],
    "no-var":"error",
    "no-unused-vars": "off"
  }
}