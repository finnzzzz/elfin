{
  "name": "vite-extension",
  "description": "extension",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_title": "extension",
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ]
}
